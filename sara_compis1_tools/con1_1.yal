
(* Archivo 1 *)

let delimitador = [' ''\t''\n']
let espacioEnBlanco = delimitador+
let letra = ['a'-'z''A'-'Z']
let digito = ['0'-'9']
let id = letra(letra|digito)*

rule tokens =
    espacioEnBlanco				{ } (* Ignorar espacios en blanco *)
  | id				{ id }
  | '+'				{ + }
  | '('				{ ( }
  | ')'				{ ) }
  | '*'				{ * }
